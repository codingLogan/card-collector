{
    "compilerOptions": {
      "module":"Node16",
      "target": "es5",
      "outDir": "./built",
      "allowJs": true,
      "noEmitOnError": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
    },
    "include": ["./src/**/*"],
}